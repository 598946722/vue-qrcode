{
  "name": "vue-qrcode",
  "description": "Qrcode component for Vue.js.",
  "version": "0.2.0",
  "main": "dist/vue-qrcode.common.js",
  "module": "dist/vue-qrcode.esm.js",
  "browser": "dist/vue-qrcode.js",
  "repository": "xkeshi/vue-qrcode",
  "license": "MIT",
  "author": "xkeshi",
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "vue-component",
    "qrcode",
    "component",
    "html",
    "css",
    "javascript",
    "front-end",
    "web",
    "development"
  ],
  "scripts": {
    "dev": "rollup -c -m -w",
    "lint": "eslint ./src",
    "build": "rollup -c",
    "compress": "uglifyjs ./dist/vue-qrcode.js -o ./dist/vue-qrcode.min.js -c -m --comments /^!/",
    "prebuild": "npm run lint",
    "postbuild": "npm run compress",
    "start": "npm run dev"
  },
  "dependencies": {
    "qrious": "^4.0.2"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.0.0",
    "uglify-js": "^3.0.15"
  },
  "peerDependencies": {
    "vue": "^2.2.0"
  }
}
